                                "def handler(event, context):",
                                "    ec2c = boto3.client('ec2')",
                                "    r = ec2c.describe_availability_zones()",
                                "    azs = r.get('AvailabilityZones')",
                                "    az_list = [az.get('ZoneName') for az in azs if az.get('State') == 'available']",
                                "    az0 = az_list[0]",
                                "    az1 = az_list[1]",
                                "    if len(az_list) > 2:",
                                "        az2 = az_list[2]",
                                "    else:",
                                "        az2 = az0",
                                "    resp = {'AZ0': az0, 'AZ1': az1, 'AZ2': az2}",
