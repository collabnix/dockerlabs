    "Conditions": {
        "CloudStorEfsSelected": {
            "Fn::Equals": [
                {
                    "Ref": "EnableCloudStorEfs"
                },
                "yes"
            ]
        },
        "CreateLogResources": {
            "Fn::Equals": [
                {
                    "Ref": "EnableCloudWatchLogs"
                },
                "yes"
